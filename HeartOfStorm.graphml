void PillIndicate() {
    Vibro(SHORT, 100);
}

void ShowHitState(QState* me) {
    if (me-&gt;CharHP &gt;= HP_OK) {
        Flash(Green, FLASH_MS);
    } else if (me-&gt;CharHP &gt;= HP_BAD) {
        Flash(Yellow, FLASH_MS);
    } else {
        Flash(Red, FLASH_MS) ;
    }
}    

void IndicateDamage(Damage) {
    Flash(Red, FLASH_MS);
    if (Damage &gt;= DAMAGE_HIGH) {
        Vibro(HARD, 300, 3);
    } else if (Damage &gt; DAMAGE_LOW) {
        Vibro(MEDIUM, 300, 2);
    } else {
        Vibro(LOW, 300, 1);
   }
   Beep();
}
 
void UpdateHP(QState* me, int HP) {
    if  (HP &lt;= me-&gt;MaxHP) {
         me-&gt;CharHP = HP;
    } else {
        me-&gt;CharHP = me-&gt;MaxHP;
    }
    SaveHP();   
}

void UpdateMaxHp(QState* me, int MaxHp) {
    me-&gt;MaxHP = MaxHP;
    SaveMaxHp();
}

void PillIndicate() {
    Flash(Blue, FLASH_MS);
    Vibro(LOW, 300, 1);
}</y:NodeLabel>
