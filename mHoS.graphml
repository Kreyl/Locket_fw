void ShowHP(MHoS* me) {
   Flash(((Color_t){255 - me-&gt;CharHP*me-&gt;ScaleStep, me&gt;CharHP*me-&gt;ScaleStep, 0}), FLASH_MS);
   
}    

void IndicateDamage(MHoS* me) {
    ShowHP(me);
    Vibro(HARD, 300, 1);
}
 
void UpdateHP(MHoS* me, int HP) {
    if  (HP &lt;= me-&gt;MaxHP) {
         me-&gt;CharHP = HP;
    } else {
         me-&gt;CharHP = me-&gt;MaxHP;
    }
    SaveHP(HP);   
}

void UpdateMaxHP(MHoS* me, int MaxHP) {
    me-&gt;MaxHP = MaxHP;
    SaveMaxHp(MaxHP);
}

void UpdateDefaultHP(MHoS* me, int DefaultHP) {
    me-&gt;DefaultHP = DefaultHP;
    SaveDefaultHP(DefaultHP);
}


void SendKillingSignal() {
        SendKillByRadio();
         Vibro(HARD, 1000, 1);
}

void Reset(MHoS* me) {
     UpdateMaxHP(me, me-&gt;DefaultHP);
     UpdateHP(me, me-&gt;DefaultHP);
     me-&gt;ScaleStep = 255/me-&gt;MaxHP;
     me-&gt;DeathTime = 0;

void Double(MHoS* me)  {
     UpdateMaxHP(me, me-&gt;DefaultHP*2);
     UpdateHP(me, me-&gt;MaxHP);
     me-&gt;DeathTime = 0;
}

